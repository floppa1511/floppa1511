local function a(b,c)b:InputHoldBegin()task.wait(c or b.HoldDuration)b:InputHoldEnd()end;while true do for d,e in pairs(game:GetService("Workspace").Map.Stages:GetDescendants())do if e:IsA("ProximityPrompt")and e.Parent.Parent.Name==tostring(...)then a(e)end end;task.wait(3)local f={[1]="Server",[2]="StageManager",[3]="Select"}local g={[1]="VSFNaF1_Beatbox",[2]="Hard"}game:GetService("ReplicatedStorage").RF:InvokeServer(f,g)task.wait(2)local f={[1]="Server",[2]="StageManager",[3]="Select"}local g={[3]="JudgementHall"}game:GetService("ReplicatedStorage").RE:FireServer(f,g)task.wait(2)local f={[1]="Server",[2]="StageManager",[3]="Select"}local g={[4]="HighestScore"}game:GetService("ReplicatedStorage").RF:InvokeServer(f,g)task.wait(56-(2+2))end
